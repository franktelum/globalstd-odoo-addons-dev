<?xml version="1.0" encoding="utf-8"?>
<openerp>
   <data>

      <record id="gpsi_contract_view_tree" model="ir.ui.view">
         <field name="name">gpsi.contract.view.tree</field>
         <field name="model">gpsi.contract</field>
         <field name="help" type="html">
             <p>
                 Click to create a new contract.
             </p>
         </field>
         <field name="arch" type="xml">
            <tree string="Contracts">
               <field name="name"/>
               <field name="standard"/>
               <field name="cycle"/>
               <field name="create_date" string="Date"/>
               <field name="need_bilingual_auditor" string="Bilingual"/>
               <field name="certification_type"/>
            </tree>
         </field>
      </record>

   </data>
</openerp>
