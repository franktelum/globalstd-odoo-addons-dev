<?xml version="1.0" encoding="utf-8"?>
<openerp>
   <data>

      <record id="gpsi_audit_standard_view_form" model="ir.ui.view">
         <field name="name">gpsi.audit.standard.form</field>
         <field name="model">gpsi.audit.standard</field>
         <field name="arch" type="xml">
            <form string="Audit Standard">
               <sheet>
                  <group>
                     <field name="name" />
                     <field name="abbreviation" />
                     <field name="description" />
                  </group>
                  <notebook>
                     <page string="IAFS">
                        <field name="iaf_ids">
                           <tree string="IAFS">
                              <field name="name"/>
                              <field name="description"/>
                           </tree>
                        </field>
                     </page>
                  </notebook>
               </sheet>
            </form>
         </field>
      </record>

   </data>
</openerp>
